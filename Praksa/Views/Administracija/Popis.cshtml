@using Praksa.Models
@model IEnumerable<Student>


@{
//Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Popis</title>
</head>
<body>
    <div> 
        <table class="table">
            <tr>
                <th>Matični Broj</th>
                <th>Ime</th>
                <th>Prezime</th>
                <th>Adresa stanovanja</th>
                <th>Mail</th>
                <th>Telefon</th>
                <th>Smjer studija</th>
                <th>Godina studija</th>
                <th>Korisničko ime</th>
                <th>Lozinka</th>
            </tr>
            @foreach (Student s in Model) {
                <tr>
                    <td>@s.maticniBroj</td>
                    <td>@s.ime</td>
                    <td>@s.prezime</td>
                    <td>@s.adresaStanovanja</td>
                    <td>@s.mail</td>
                    <td>@s.telefon</td>
                    <td>@s.smjerStudija</td>
                    <td>@s.godinaStudija</td>
                    <td>@s.korisnickoIme</td>
                    <td>@s.lozinka</td>
                </tr>
            }
        </table>    
    </div>
</body>
</html>
